{
  "name": "apperitif",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:offline": "expo start --offline",
    "start:https": "expo start --https",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --https",
    "test": "jest --watchAll",
    "format": "prettier --write \"./**/*.{js,jsx,json,md,ts,tsx}\"",
    "compile": "tsc --noEmit -p . --pretty",
    "add:test-device": "eas device:create",
    "build:run:simulator": "eas build:run -p ios --path",
    "build:dev:ios": "eas build --profile development --platform ios --local",
    "build:dev:ios-simulator": "eas build --profile development-simulator --platform ios --local",
    "build:dev:android": "eas build --profile development --platform android --local",
    "build:staging:ios": "eas build --profile staging --platform ios --local",
    "build:staging:android": "eas build --profile staging --platform android --local",
    "build:production:ios": "eas build --profile production --platform ios --local",
    "build:production:android": "eas build --profile production --platform android --local",
    "build-and-submit:production:ios": "eas build --profile production --platform ios --auto-submit",
    "build-and-submit:production:android": "eas build --profile production --platform android --auto-submit",
    "expo:doctor": "npx expo-doctor@latest",
    "expo:fix": "npx expo install --fix",
    "generate:types": "npx supabase gen types typescript --project-id kvtfspymzqsyhmvkzgej --schema public > src/types/supabase.ts",
    "generate:gql": "graphql-codegen",
    "generate:gqlwatch": "graphql-codegen -w"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/native": "^6.0.2",
    "@supabase/supabase-js": "^2.38.0",
    "aes-js": "^3.1.2",
    "apollo3-cache-persist": "^0.14.1",
    "expo": "~49.0.13",
    "expo-apple-authentication": "~6.1.0",
    "expo-application": "~5.3.0",
    "expo-auth-session": "~5.0.2",
    "expo-blur": "~12.4.1",
    "expo-crypto": "~12.4.1",
    "expo-dev-client": "~2.4.11",
    "expo-font": "~11.4.0",
    "expo-haptics": "~12.4.0",
    "expo-image": "~1.3.4",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.16",
    "expo-web-browser": "~12.3.2",
    "lodash": "^4.17.21",
    "lodash-humps": "^3.1.6",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-big-list": "^1.6.1",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-get-random-values": "^1.9.0",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-shimmer-placeholder": "^2.0.9",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@expo/webpack-config": "^19.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/aes-js": "^3.1.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/react": "~18.2.14",
    "@types/react-native-get-random-values": "^1.8.0",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.2.2",
    "typescript": "^5.1.3",
    "uri-scheme": "^1.1.0"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "importOrder": [
      "^~/(.*)$",
      "^[./]"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "no-undef": 0
    }
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "private": true
}

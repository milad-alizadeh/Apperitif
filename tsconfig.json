{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "~assets/*": ["./assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/app/(tabs)/profile/_layout.",
    "app.config.js",
    "src/components/Accordon/index.test.tss"
  ]
}
